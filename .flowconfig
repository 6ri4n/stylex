[version]
0.162.1

[ignore]
.*/build/.*
.*/dist/.*
.*/lib/.*
.*/node_modules/.*

[include]

[libs]
flow-typed

[lints]

[options]
emoji=true
exact_by_default=true
experimental.const_params=true
module.use_strict=true
munge_underscores=true
suppress_type=$FlowFixMe
suppress_type=$FlowTODO
module.name_mapper='^@stylexjs\/shared\/\([a-zA-Z0-9_\-]+\)$' -> '<PROJECT_ROOT>/packages/shared/src/\1'

[strict]
